{
    "name": "github-php/sponsors",
    "description": "A package for PHP to work with GitHub Sponsors.",
    "keywords": ["GitHub", "Sponsors", "Laravel"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/github-php/sponsors/issues",
        "source": "https://github.com/github-php/sponsors"
    },
    "authors": [
        {
            "name": "Dries Vints",
            "homepage": "https://driesvints.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/http": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dries\\GitHubSponsors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "GitHubSponsors": "Dries\\GitHubSponsors\\Facades\\GitHubSponsorsFacade"
            },
            "providers": [
                "Dries\\GitHubSponsors\\GitHubSponsorsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
